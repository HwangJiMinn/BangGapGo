{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://firstandbest.shop/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://firstandbest.shop/:path*/"
    },
    {
      "routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }]
    }
  ]
}
